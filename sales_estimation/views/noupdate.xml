<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_crm_quote_code" model="ir.sequence">
            <field name="name">CRM Code</field>
            <field name="code">crm.quote.code</field>
            <field name="prefix">ENQ</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_estimation_code" model="ir.sequence">
            <field name="name">Estimation Code</field>
            <field name="code">estimation.code</field>
            <field name="prefix">EST</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_quotation_code" model="ir.sequence">
            <field name="name">Estimation Code</field>
            <field name="code">quotation.code</field>
            <field name="prefix">QO</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>